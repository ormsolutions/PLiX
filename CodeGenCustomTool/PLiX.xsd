			<xs:element name="typeparam" type="docCommentType_param" form="unqualified" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="param" type="docCommentType_param" form="unqualified" minOccurs="0" maxOccurs="unbounded"/>
